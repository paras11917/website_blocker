{
    "manifest_version": 3,
    "name": "Website Blocker",
    "description": "Website blocker is an extension which allow you to block some websites .",
    "version": "1.0",
    "icons": {
      "128": "./banner.png",
      "48": "./banner.png",
      "16": "./banner.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
      }
    ]
  }
